}], "MathCaption",
 CellTags->"Form"],

Cell[BoxData[
 RowBox[{
  RowBox[{"newZ", "[", 
   RowBox[{"n_Integer", "?", "Positive"}], "]"}], " ", ":=", "\n", "\t\t", 
  RowBox[{"FormGroupoid", "[", 
   RowBox[{
    RowBox[{"Range", "[", 
     RowBox[{"0", ",", 
      RowBox[{"n", "-", "1"}]}], "]"}], ",", " ", 
    RowBox[{
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{"#1", " ", "+", " ", "#2"}], ",", "n"}], "]"}], "&"}], ",", 
    "\"\<+\>\"", ",", 
    RowBox[{"IsAGroup", " ", "\[Rule]", " ", "True"}], ",", " ", 
    RowBox[{"FormatOperator", " ", "\[Rule]", "False"}], ",", " ", 
    RowBox[{"Generators", " ", "\[Rule]", " ", 
     RowBox[{"{", "1", "}"}]}], ",", "\n", "\t", 
    RowBox[{
    "GroupoidDescription", " ", "\[Rule]", " ", 
     "\"\<Integers under addition mod n\>\""}], ",", 
    RowBox[{"GroupoidName", " ", "\[Rule]", 
     RowBox[{"StringJoin", "[", 
      RowBox[{"\"\<Z[\>\"", ",", 
