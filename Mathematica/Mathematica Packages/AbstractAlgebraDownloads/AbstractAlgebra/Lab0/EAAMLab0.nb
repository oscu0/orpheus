 CellTags->{"a:0.8.1.2.0", "Lab0"}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"IllustrateModReduction", "[", 
    RowBox[{"a_Integer", ",", 
     RowBox[{"n_Integer", "?", "Positive"}], ",", "opts___"}], "]"}], ":=", 
   RowBox[{"Show", "[", 
    RowBox[{"computeModReduction", "[", 
     RowBox[{"a", ",", "n", ",", "opts"}], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"IllustrateModReduction", "[", 
   RowBox[{"a_", ",", "n_", ",", "opts___"}], "]"}], ":=", 
  RowBox[{"Message", "[", 
   RowBox[{"IllustrateModReduction", "::", "\"\<arg\>\""}], "]"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "computeModReduction", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"showText", "\[Rule]", "True"}], ",", 
     RowBox[{"dotColor", "\[Rule]", "Blue"}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"textPortionOfReduction", "[", 
   RowBox[{"a_", ",", "n_", ",", "r_", ",", "q_"}], "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"a", ",", "\"\< mod \>\"", ",", "n", ",", "\"\< is \>\"", ",", 
      RowBox[{"Mod", "[", 
       RowBox[{"a", ",", "n"}], "]"}], ",", "\"\< since \>\"", ",", "a", ",", 
      "\"\< = \>\"", ",", 
      RowBox[{"Quotient", "[", 
       RowBox[{"a", ",", "n"}], "]"}], ",", "\"\<*\>\"", ",", "n", ",", 
      "\"\< + \>\"", ",", 
      RowBox[{"Mod", "[", 
       RowBox[{"a", ",", "n"}], "]"}], ",", "\"\<.\>\""}], "]"}], ";", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<In other words, \>\"", ",", "a", ",", "\"\< mod \>\"", ",", "n", 
      ",", "\"\< is the remainder \>\""}], "]"}], ";", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<on  division of \>\"", ",", "a", ",", "\"\< by \>\"", ",", "n", 
      ",", "\"\<.\>\""}], "]"}], ";", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"a", ">", "0"}], ",", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{"\"\<To illustrate, \>\"", ",", "\"\<below \>\"", ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"q", "==", "1"}], ",", "\"\<is \>\"", ",", 
           "\"\<are \>\""}], "]"}], ",", "q", ",", "\"\< full column\>\"", 
         ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"q", "=!=", "1"}], ",", "\"\<s\>\"", ",", "\"\<\>\""}], 
          "]"}]}], "]"}], ";", 
       RowBox[{"Print", "[", 
        RowBox[{"\"\<of height \>\"", ",", "n", ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"r", ">", "0"}], ",", 
           "\"\< and one column with height \>\"", ",", "\"\<\>\""}], "]"}], 
         ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"r", ">", "0"}], ",", "r", ",", "\"\<\>\""}], "]"}], ",", 
         "\"\<.\>\""}], "]"}], ";", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"r", "==", "0"}], ",", 
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<Since there is no remainder, \>\"", ",", "a", ",", 
           "\"\< mod \>\"", ",", "n", ",", "\"\< is 0.\>\""}], "]"}], ",", 
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<Since the remainder is \>\"", ",", "r", ",", "\"\<, \>\"", ",",
            "a", ",", "\"\< mod \>\"", ",", "n", ",", "\"\< is \>\"", ",", 
           "r", ",", "\"\<.\>\""}], "]"}]}], "]"}]}]}], "]"}]}], 
   ")"}]}], "\n", 
 RowBox[{
  RowBox[{"computeModReduction", "[", 
   RowBox[{"a_", ",", "n_", ",", "opts___"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"r", ",", "q", ",", "showtext", ",", "dotcolor"}], "}"}], ",", 
    RowBox[{
     RowBox[{"SetOptions", "[", 
      RowBox[{"Graphics", ",", 
       RowBox[{"AspectRatio", "\[Rule]", 
        FractionBox["1", "GoldenRatio"]}]}], "]"}], ";", 
     RowBox[{"r", "=", 
      RowBox[{"Mod", "[", 
       RowBox[{"a", ",", "n"}], "]"}]}], ";", 
     RowBox[{"q", "=", 
      RowBox[{"Quotient", "[", 
       RowBox[{"a", ",", "n"}], "]"}]}], ";", 
     RowBox[{"dotcolor", "=", 
      RowBox[{
       RowBox[{"dotColor", "/.", 
        RowBox[{"{", "opts", "}"}]}], "/.", 
       RowBox[{"Options", "[", "computeModReduction", "]"}]}]}], ";", 
     RowBox[{"showtext", "=", 
      RowBox[{
       RowBox[{"showText", "/.", 
        RowBox[{"{", "opts", "}"}]}], "/.", 
       RowBox[{"Options", "[", "computeModReduction", "]"}]}]}], ";", 
     RowBox[{"If", "[", 
      RowBox[{"showtext", ",", 
       RowBox[{"textPortionOfReduction", "[", 
        RowBox[{"a", ",", "n", ",", "r", ",", "q"}], "]"}]}], "]"}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"q", "==", "0"}], ",", 
       RowBox[{"makeDots", "[", 
        RowBox[{
        "1", ",", "1", ",", "r", ",", "dotcolor", ",", "Yellow", ",", 
         "0.025"}], "]"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"r", "==", "0"}], ",", 
         RowBox[{"makeDots", "[", 
          RowBox[{
          "1", ",", "q", ",", "n", ",", "dotcolor", ",", "Red", ",", "0.025", 
           ",", 
           RowBox[{"colLabels", "\[Rule]", "True"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"makeDots", "[", 
            RowBox[{
            "1", ",", "q", ",", "n", ",", "dotcolor", ",", "Red", ",", 
             "0.025", ",", 
             RowBox[{"colLabels", "\[Rule]", "True"}]}], "]"}], ",", 
           RowBox[{"makeDots", "[", 
            RowBox[{
             RowBox[{"q", "+", "1.5"}], ",", "1", ",", "r", ",", "dotcolor", 
             ",", "Yellow", ",", "0.025"}], "]"}]}], "}"}]}], "]"}]}], 
      "]"}]}]}], "]"}]}]}], "Input",
 InitializationCell->True,
 CellTags->"Lab0"]
}, Closed]],

