Cell[BoxData[{
 RowBox[{
  RowBox[{"IllustrateModAddition", "[", 
   RowBox[{"a_Integer", ",", "b_Integer", ",", 
    RowBox[{"n_Integer", "?", "Positive"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "ra", ",", "rb", ",", "qa", ",", "qb", ",", "first", ",", "mid", ",", 
      "mid2", ",", "end", ",", "seq"}], "}"}], ",", 
    RowBox[{
     RowBox[{"SetOptions", "[", 
      RowBox[{"Graphics", ",", 
       RowBox[{"AspectRatio", "\[Rule]", 
        FractionBox["1", "GoldenRatio"]}]}], "]"}], ";", 
     RowBox[{"ra", "=", 
      RowBox[{"Mod", "[", 
       RowBox[{"a", ",", "n"}], "]"}]}], ";", 
     RowBox[{"qa", "=", 
      RowBox[{"Quotient", "[", 
       RowBox[{"a", ",", "n"}], "]"}]}], ";", 
     RowBox[{"rb", "=", 
      RowBox[{"Mod", "[", 
       RowBox[{"b", ",", "n"}], "]"}]}], ";", 
     RowBox[{"qb", "=", 
      RowBox[{"Quotient", "[", 
       RowBox[{"b", ",", "n"}], "]"}]}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<We wish to calculate \>\"", ",", "a", ",", "\"\< mod \>\"", ",", 
       "n", ",", "\"\<  +  \>\"", ",", "b", ",", "\"\< mod \>\"", ",", "n", 
       ",", "\"\<.\>\""}], "]"}], ";", 
     RowBox[{
     "Print", "[", 
      "\"\<To do so, we determine the quotients and remainders when\>\"", 
      "]"}], ";", 
     RowBox[{"Print", "[", 
      RowBox[{
      "a", ",", "\"\< and \>\"", ",", "b", ",", "\"\< are divided by \>\"", 
       ",", "n", ",", "\"\<.\>\""}], "]"}], ";", 
     RowBox[{"first", "=", 
      RowBox[{"{", 
       RowBox[{"showStageOne", "[", 
        RowBox[{"ra", ",", "rb", ",", "qa", ",", "qb", ",", "n"}], "]"}], 
       "}"}]}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"qa", "+", "qb"}], ">", 
         RowBox[{"Max", "[", 
          RowBox[{"qa", ",", "qb"}], "]"}]}], "&&", 
        RowBox[{
         RowBox[{"ra", "+", "rb"}], ">", 
         RowBox[{"Max", "[", 
          RowBox[{"ra", ",", "rb"}], "]"}]}]}], ",", 
       RowBox[{"mid", "=", 
