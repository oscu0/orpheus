Cell["4.4.1 Surjectivity and injectivity", "Subsubsection",
 CellMargins->{{Inherited, Inherited}, {Inherited, 8}},
 CellTags->"SurjectivityInjectivity"],

Cell[TextData[{
 "There are two standard properties of functions that are often of interest. \
A function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", " ", ":", " ", "A"}], " ", 
    StyleBox["\[Rule]",
     FontFamily->"Courier"], " ", "B"}], TraditionalForm]]],
 " is said to be ",
 StyleBox["injective",
  FontSlant->"Italic"],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "x", ")"}], " ", "=", " ", 
    RowBox[{"f", "(", "y", ")"}]}], TraditionalForm]]],
 " implies ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", " ", "=", " ", "y"}], TraditionalForm]]],
 " and ",
 StyleBox["surjective",
  FontSlant->"Italic"],
 " if for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", " ", "\[Element]", " ", "B"}], TraditionalForm]]],
 " there exists an ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", " ", "\[Element]", " ", "A"}], TraditionalForm]]],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "a", ")"}], " ", "=", " ", "b"}], TraditionalForm]]],
 "."
}], "Text",
 CellTags->"SurjectivityInjectivity"],

