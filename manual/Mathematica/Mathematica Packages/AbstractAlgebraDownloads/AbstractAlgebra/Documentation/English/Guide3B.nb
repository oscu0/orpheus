Cell[CellGroupData[{

Cell["3.6.2 Subrings and ideals", "Subsubsection",
 CellTags->{"Subrings", "i:7"}],

Cell[TextData[{
 "When the ringoid ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]]],
 " is a ring, the expression ",
 StyleBox["SubringQ", "MR"],
 "[",
 StyleBox["S, R",
  FontSlant->"Italic"],
 "] tests whether a list of elements ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]]],
 " is a subring of ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]]],
 ". This is done by testing whether ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]]],
 " is nonempty and closed with respect to addition and multiplication in ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]]],
 ". Subrings must also contain negations of its elements, but in a finite \
ring, it is sufficient to verify closure to conclude that this property is \
true."
}], "Text",
 CellTags->"Subrings"],

Cell[TextData[{
 "Given a subring ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]]],
 ", we call it a two-sided ",
 StyleBox["ideal",
  FontSlant->"Italic"],
 " of the ring ",
 Cell[BoxData[
  FormBox["R", TraditionalForm]]],
 " if for every ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "\[Element]", "R"}], TraditionalForm]]],
 " and every ",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "\[Element]", "S"}], TraditionalForm]]],
 " both ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "s"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"s", "r"}], TraditionalForm]]],
 " are in ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]]],
 ". Left ideals and right ideals require only one of these products to be in \
",
 Cell[BoxData[
  FormBox["S", TraditionalForm]]],
 "."
}], "Text",
 CellTags->{"Subrings", "i:89"}],

