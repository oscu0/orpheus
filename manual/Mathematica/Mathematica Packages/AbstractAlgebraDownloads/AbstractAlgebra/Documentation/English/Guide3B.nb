Cell[TextData[{
 "Consider the element ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "x"}], TraditionalForm]]],
 " in ",
 StyleBox["Q2", "MR"],
 ". Note that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"2", "x"}], ")"}], "2"], "+", "2"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"4", 
       SuperscriptBox["x", "2"]}], "+", "2"}], "=", 
     RowBox[{
      SuperscriptBox["x", "2"], "+", "2"}]}]}], TraditionalForm]]],
 ". Therefore ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "x"}], TraditionalForm]]],
 " is a root (zero) of the polynomial ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["x", "2"], "+", "2"}], TraditionalForm]]],
 " over ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[DoubleStruckCapitalZ]", "3"], TraditionalForm]]],
 ". The following function confirms this."
}], "MathCaption",
 CellTags->"QuotientPolynomials"],

