    \(TableForm[
      CartesianProduct[{"\<a\>", "\<b\>", "\<c\>", "\<d\>"}, {1, 2, 3}, \ 
        Partition\  \[Rule] \ True], \ TableDepth\  \[Rule] \ 1]\)], "Input",
  CellLabel->"In[83]:=",
  ShowCellBracket->True,
  CellTags->"WorkLists"],

