 ShowCellBracket->True,
 CellLabel->"In[39]:="],

Cell[TextData[{
 "The ",
 StyleBox["Visual", "MR"],
 " mode is intended to illustrate how this coset is derived."
}], "MathCaption",
 ShowCellBracket->True],

