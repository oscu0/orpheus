  ShowCellBracket->True,
  CellTags->"AdditiveP"],

Cell[BoxData[
    \({ClosedQ[Z[5], \ Operation\  \[Rule] \ Addition], \ HasZeroQ[Z[5]], \ 
      HasInversesQ[Z[5], \ Operation\  \[Rule] \ Addition], \ 
      AssociativeQ[Z[5], \ Operation\  \[Rule] \ Addition], \ 
      CommutativeQ[Z[5], \ Operation\  \[Rule] \ Addition]}\)], "Input",
