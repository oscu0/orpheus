  ShowCellBracket->True,
  CellTags->"FiniteFields"],

Cell[BoxData[
    RowBox[{
      StyleBox["AdditiveToMultiplicative",
        FontFamily->"Courier",
        FontWeight->"Bold"], 
      StyleBox["[",
